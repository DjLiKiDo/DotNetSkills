{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Error"
    }
  },
  "_SecurityNote": "All secrets must be provided via environment variables or Azure Key Vault. Never commit secrets to source control.",
  "Database": {
    "ConnectionString": "",
    "EnableSensitiveDataLogging": false,
    "EnableDetailedErrors": false,
    "EnableQueryLogging": false
  },
  "FeatureFlags": {
    "EnableBetaFeatures": false
  },
  "KeyVault": {
    "Uri": "https://your-keyvault-name.vault.azure.net/"
  },
  "Swagger": {
    "Enabled": true
  },
  "Cors": {
    "PolicyName": "Default",
    "AllowedOrigins": ["https://app.dotnetskills.com"],
    "AllowCredentials": true
  },
  "Jwt": {
    "Enabled": true,
    "Issuer": "https://app.dotnetskills.com",
    "Audience": "dotnetskills-api",
    "SigningKey": "",
    "TokenLifetimeMinutes": 60
  }
}
