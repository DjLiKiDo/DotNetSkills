{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Error"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=prod-sql;Database=DotNetSkills;User Id=prod_user;Password=prod_pass;TrustServerCertificate=true"
  },
  "Database": {
    "EnableSensitiveDataLogging": false,
    "EnableDetailedErrors": false,
    "EnableQueryLogging": false
  },
  "FeatureFlags": {
    "EnableBetaFeatures": false
  },
  "KeyVault": {
    "Uri": "https://your-keyvault-name.vault.azure.net/"
  },
  "Swagger": {
    "Enabled": true
  },
  "Cors": {
    "PolicyName": "Default",
    "AllowedOrigins": ["https://app.dotnetskills.com"],
    "AllowCredentials": true
  },
  "Jwt": {
    "Enabled": true,
    "Issuer": "https://app.dotnetskills.com",
    "Audience": "dotnetskills-api"
  }
}
