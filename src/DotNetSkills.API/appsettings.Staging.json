{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Information"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=staging-sql;Database=DotNetSkills_Staging;User Id=staging_user;Password=staging_pass;TrustServerCertificate=true"
  },
  "Database": {
    "EnableSensitiveDataLogging": false,
    "EnableDetailedErrors": false,
    "EnableQueryLogging": false
  },
  "FeatureFlags": {
    "EnableBetaFeatures": true
  },
  "KeyVault": {
    "Uri": ""
  },
  "Swagger": {
    "Enabled": true
  },
  "Cors": {
    "PolicyName": "Default",
    "AllowedOrigins": ["https://staging.dotnetskills.com"],
    "AllowCredentials": true
  },
  "Jwt": {
    "Enabled": true,
    "Issuer": "https://staging.dotnetskills.com",
    "Audience": "dotnetskills-api"
  }
}
